{
  "name": "moviesApp",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@material-ui/core": "^4.11.2",
    "@material-ui/icons": "^4.11.2",
    "@material-ui/lab": "^4.0.0-alpha.57",
    "prop-types": "^15.7.2",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-hook-form": "^3.29.4",
    "react-query": "^3.13.0",
    "react-router-dom": "^5.2.0",
    "react-scripts": "4.0.1"
  },
  "scripts": {
    "build": "npm-run-all clean compile:all",
    "clean": "rimraf ./build && mkdir build",
    "compile:moviemodels": "babel ./api/movies --out-dir build/api/movies",
    "compile:usermodels": "babel ./api/users --out-dir build/api/users",
    "compile:genremodels": "babel ./api/genres --out-dir build/api/genres",
    "compile:models": "npm-run-all compile:moviemodels compile:usermodels compile:genremodels",
    "compile:authenticate": "babel ./authenticate --out-dir build/authenticate",
    "compile:seed": "babel ./seedData --out-dir build/seedData",
    "compile:app": "babel index.js --out-dir build/",
    "compile:db": "babel ./db --out-dir build/db",
    "copy:nodeModules": "ncp ./node_modules/ build/node_modules",
    "copy:public": "ncp ./public build/public",
    "copy:all": "npm-run-all copy:nodeModules copy:public",
    "compile:all": "npm-run-all compile:models compile:seed compile:authenticate compile:app compile:db copy:all",
    "postinstall": "rimraf ./build && npm run compile:all",
    "start": "react-scripts start",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "build-storybook": "build-storybook -s public"
  },
  "eslintConfig": {
    "extends": "react-app",
    "overrides": [
      {
        "files": [
          "**/*.stories.*"
        ],
        "rules": {
          "import/no-anonymous-default-export": "off"
        }
      }
    ]
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "devDependencies": {
    "@storybook/addon-actions": "^6.2.9",
    "@storybook/addon-links": "^6.2.9",
    "@storybook/node-logger": "^6.2.9",
    "@storybook/preset-create-react-app": "^3.1.7",
    "@storybook/react": "^6.2.9",
    "cypress": "^8.7.0",
    "http-server": "^13.0.2",
    "start-server-and-test": "^1.14.0"
  },
  "proxy":"http://localhost:8080"
}
